    <!--#region Tab Menu-->

    <Style x:Key="RadioButtonTabMenuStyle" TargetType="RadioButton">
        <Setter Property="Background" Value="{DynamicResource BaseColorTheme}" />
        <Setter Property="Cursor" Value="{StaticResource BaseCursor}" />
        <Setter Property="FontFamily" Value="{DynamicResource BaseFontFamily}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RadioButton">
                    <Border
                        Width="150"
                        Height="29"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="12"
                        IsHitTestVisible="{TemplateBinding IsHitTestVisible}">
                        <ContentPresenter
                            HorizontalAlignment="Center"
                            VerticalAlignment="Center"
                            Content="{TemplateBinding Content}" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>

            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Background" Value="#22202f" />
                <Setter Property="Foreground" Value="White" />
            </Trigger>

        </Style.Triggers>
    </Style>

    <!--#endregion-->

    <!--#region Menu-->

    <!--#region General Menu-->

    <Storyboard x:Key="GeneralMenuButtonOnChecked">
        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)">
            <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFFEA700" />
        </ColorAnimationUsingKeyFrames>
    </Storyboard>
    <Storyboard x:Key="GeneralMenuButtonOnUnchecked">
        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)">
            <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#22202f" />
        </ColorAnimationUsingKeyFrames>
    </Storyboard>

    <Style
        x:Key="GeneralMenuButtonStyle"
        BasedOn="{StaticResource {x:Type ToggleButton}}"
        TargetType="{x:Type RadioButton}">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="RadioButton">

                        <Border
                            Margin="2"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Stretch"
                            Background="{TemplateBinding Background}"
                            CornerRadius="10">

                            <TextBlock
                                Margin="50,0,0,0"
                                VerticalAlignment="Center"
                                FontFamily="{DynamicResource BaseFontFamily}"
                                Foreground="{TemplateBinding Foreground}"
                                Text="{TemplateBinding Property=Content}" />
                        </Border>

                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Cursor" Value="Hand" />
        </Style.Setters>
        <Style.Triggers>
            <EventTrigger RoutedEvent="ToggleButton.Checked">
                <BeginStoryboard Storyboard="{DynamicResource GeneralMenuButtonOnChecked}" />
            </EventTrigger>
            <EventTrigger RoutedEvent="ToggleButton.Unchecked">
                <BeginStoryboard Storyboard="{DynamicResource GeneralMenuButtonOnUnchecked}" />
            </EventTrigger>
        </Style.Triggers>

    </Style>

