    <Storyboard x:Key="BigButtonMouseEnter">
        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Background).(SolidColorBrush.Color)">
            <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FFFEA700" />
        </ColorAnimationUsingKeyFrames>
    </Storyboard>
    <Storyboard x:Key="BigButtonMouseLeave">
        <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Background).(SolidColorBrush.Color)">
            <EasingColorKeyFrame KeyTime="0:0:0.4" Value="White" />
        </ColorAnimationUsingKeyFrames>
    </Storyboard>

    <Style x:Key="BigMetroMenuButton" TargetType="Button">
        <Setter Property="Cursor" Value="{StaticResource BaseCursor}" />
        <Setter Property="Background" Value="{StaticResource BaseColorBackground}" />
        <Style.Triggers>
            <EventTrigger RoutedEvent="Button.MouseEnter">
                <BeginStoryboard Storyboard="{StaticResource BigButtonMouseEnter}" />
            </EventTrigger>
            <EventTrigger RoutedEvent="Button.MouseLeave">
                <BeginStoryboard Storyboard="{StaticResource BigButtonMouseLeave}" />
            </EventTrigger>
        </Style.Triggers>
    </Style>

